<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
	<Script file="QuestItem.lua"/>
	<Script file="QuestItemFunctions.lua" />
	<Script file="QuestItemTooltip.lua" />
	<Frame name="QuestItem" parent="UIParent" hidden="true">
		<Scripts>
			<OnLoad>
				QuestItem_OnLoad();
			</OnLoad>
			<OnEvent>
				QuestItem_OnEvent(event);
			</OnEvent>
		</Scripts>
	</Frame>
		<Button name="QuestItemItemButtonTemplate" virtual="true">
		<Size>
			<AbsDimension x="300" y="16"/>
		</Size>
		<Scripts>
			<OnLoad>
				this:RegisterForClicks("LeftButtonUp", "RightButtonUp");
			</OnLoad>
			<OnEnter>
				QuestItemItemButton_OnEnter();
			</OnEnter>

		</Scripts>
		<NormalText name="$parentText" inherits="GameFontNormal" justifyH="LEFT">
			<Size>
				<AbsDimension x="275" y="12"/>
			</Size>
			<Anchors>
				<Anchor point="LEFT">
					<Offset>
						<AbsDimension x="20" y="0"/>
					</Offset>
				</Anchor>
			</Anchors> 
		</NormalText>
	</Button>
	<GameTooltip name="QuestItemTooltip" inherits="GameTooltipTemplate" parent="UIParent" hidden="true"/>
	<GameTooltip name="QIHiddenTooltip" inherits="GameTooltipTemplate" parent="UIParent" hidden="true"/>
</Ui>